{
  "manifest_version": 2,
  "name": "FPL League Extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
          "http://fantasy.premierleague.com/my-leagues/*/standings/"
        ],
      "js": ["jquery-2.1.4.min.js", "content.js"]
    }
  ],
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
   "http://fantasy.premierleague.com/"
   ]
}